
<div>
  <h2>配送先編集</h2>

  <%= form_with model: @address do |f|  %>
   <h4>郵便番号(ハイフンなし)</h4>
    <%= f.text_field :postal_code %>
   <h4>住所</h4>
    <%= f.text_field :address, size: "50" %>
   <h4>宛名</h4>
    <%= f.text_field :name %>
   <%= f.submit '変更内容を保存', class: "btn btn-success" %>
  <% end %>


</div>